    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "dates = pd.date_range(\"2024-01-01\", periods=10)\n",
    "temperatures = [22, 23, 21, 24, 25, 26, 24, 23, 22, 21]\n",
    "\n",
    "df = pd.DataFrame({\n",
    "    \"date\": dates,\n",
    "    \"temperature\": temperatures\n",
    "})\n",
    "plt.plot(df[\"date\"], df[\"temperature\"])\n",
    "plt.show()\n"
   ]
  }
